<TOKEN id="token-3-1" pos="punct" morph="none" start_char="97" end_char="97">：</TOKEN>
<TOKEN id="token-3-2" pos="word" morph="none" start_char="98" end_char="111">尼泊尔撤侨再显部分国人之丑陋</TOKEN>
</SEG>
