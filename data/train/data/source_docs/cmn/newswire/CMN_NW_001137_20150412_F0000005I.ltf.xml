      <SEG id="segment-5" start_char="206" end_char="219">
        <ORIGINAL_TEXT>希拉里正式宣布：参选美国总统</ORIGINAL_TEXT>
        <TOKEN id="token-5-0" pos="word" morph="none" start_char="206" end_char="212">希拉里正式宣布</TOKEN>
        <TOKEN id="token-5-1" pos="punct" morph="none" start_char="213" end_char="213">：</TOKEN>
        <TOKEN id="token-5-2" pos="word" morph="none" start_char="214" end_char="219">参选美国总统</TOKEN>
      </SEG>
      <SEG id="segment-6" start_char="221" end_char="231">
        <ORIGINAL_TEXT>&lt;/HEADLINE&gt;</ORIGINAL_TEXT>
        <TOKEN id="token-6-0" pos="unknown" morph="none" start_char="221" end_char="231">&lt;/HEADLINE&gt;</TOKEN>
